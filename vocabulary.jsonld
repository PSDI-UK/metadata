	"skos:Concept": [
		{
			"@id":"psdiVoc:psdi",
			"@type":"skos:Concept",
			"skos:prefLabel":"Physical Sciences Data Infrastructure",
			"skos:altLabel":"PSDI",
			"skos:definition":"An Integrated Data Infrastructure* for the Physical Sciences in the UK that brings together and builds upon the various data systems researchers currently use. *'IT Infrastructure' - 'All of the hardware, software, networks, facilities etc. that are required to develop, test, deliver, monitor, control or learning material applications and IT services' [ITIL Glossary of Terms English v.1.0]",
			"skos:related":[
				"psdiVoc:psdiProject",
				"psdiVoc:psdiDeliverable"
			],
			"skos:Collection":"psdiVoc:psdiOverviewCollection",
			"skos:inScheme":"psdiVoc:vocabularyConceptScheme",
			"skos:topConceptOf":"psdiVoc:vocabularyConceptScheme"
		},
		{
			"@id":"psdiVoc:psdiProject",
			"@type":"skos:Concept",
			"skos:prefLabel":"PSDI project",
			"skos:definition":"The project to produce the Physical Sciences Data Infrastructure, its PSDI platform and community activities.",
			"rdf:type":"foaf:Project",
			"skos:related":[
				"psdiVoc:psdi",
				"psdiVoc:pathfinder",
				"psdiVoc:psdiDeliverable"
			],
			"skos:Collection":"psdiVoc:psdiOverviewCollection",
			"skos:inScheme":"psdiVoc:vocabularyConceptScheme",
			"skos:topConceptOf":"psdiVoc:vocabularyConceptScheme"
		},
		{
			"@id":"psdiVoc:pathfinder",
			"@type":"skos:Concept",
			"skos:prefLabel":"Pathfinder",
			"skos:definition":"Early deployment project which is part of the wider PSDI project, to seed the PSDI platform with exemplar resources.",
			"rdf:type":"foaf:Project",
			"skos:related":[
				"psdiVoc:psdiProject",
				"psdiVoc:psdiDeliverable",
				"psdiVoc:resource"
			],
			"skos:Collection":"psdiVoc:psdiOverviewCollection",
			"skos:inScheme":"psdiVoc:vocabularyConceptScheme",
			"skos:topConceptOf":"psdiVoc:vocabularyConceptScheme"
		},
		{
			"@id":"psdiVoc:psdiDeliverable",
			"@type":"skos:Concept",
			"skos:prefLabel":"PSDI deliverable",
			"skos:definition":"Top-level deliverable* of the PSDI project. *' Deliverable, N.' - 'Chiefly in plural. Something which may be delivered or provided; spec. a tangible result of a development process.' [Oxford English Dictionary, Oxford UP, July 2023, https://doi.org/10.1093/OED/6700491441]",
			"skos:broader":[
				"psdiVoc:psdiPlatform",
				"psdiVoc:service",
				"psdiVoc:tool",
				"psdiVoc:data",
				"psdiVoc:guidance",
				"psdiVoc:metadata",
				"psdiVoc:resource",
				"psdiVoc:communityActivity"
			],
			"skos:related":[
				"psdiVoc:psdi",
				"psdiVoc:psdiProject",
				"psdiVoc:pathfinder"
			],
			"skos:Collection":"psdiVoc:psdiOverviewCollection",
			"skos:topConceptOf":"psdiVoc:vocabularyConceptScheme",
			"skos:inScheme":"psdiVoc:vocabularyConceptScheme"
		},
		{
			"@id":"psdiVoc:psdiPlatform",
			"@type":"skos:Concept",
			"skos:prefLabel":"PSDI platform",
			"skos:definition":"The PSDI platform* is the interface which sits on the Physical Sciences Data Infrastructure and provides common services, tools, metadata and guidance for the various resources in it. *'Platform, N., Sense I.11.a.' - 'A standard system architecture; a (type of) machine and/or operating system, regarded as the base on which software applications are run.' [Oxford English Dictionary, Oxford UP, March 2024, https://doi.org/10.1093/OED/1049097135]",
			"skos:narrower":[
				"psdiVoc:psdiDeliverable"
			],
			"skos:Collection":"psdiVoc:psdiOverviewCollection",
			"skos:inScheme":"psdiVoc:vocabularyConceptScheme"
		},
		{
			"@id":"psdiVoc:service",
			"@type":"skos:Concept",
			"skos:prefLabel":"service",
			"rdf:type":"dcat:DataService",
			"skos:definition":"Each service is software - either used directly within the PSDI platform itself or within a web application accessed from it - that facilitates access to or performs data processing functions on data (available through the PSDI platform or other data). Some services are PSDI-wide and used within different resources and some are specific to a particular resource.",
			"skos:example":[
				"user registration",
				"access management",
				"structure searching",
				"format conversion service"
			],
			"skos:narrower":[
				"psdiVoc:psdiDeliverable"
			],
			"skos:Collection":"psdiVoc:psdiOverviewCollection",
			"skos:inScheme":"psdiVoc:vocabularyConceptScheme"
		},
		{
			"@id":"psdiVoc:tool",
			"@type":"skos:Concept",
			"skos:prefLabel":"tool",
			"skos:definition":"A tool is software* - downloaded from the PSDI platform and run via one’s own non-PSDI infrastructure - that facilitates access to or performs data processing functions on data (available through the PSDI platform or other data). *'Software' - 'Computer programs, procedures, and possibly associated documentation and data pertaining to the operation of a computer System.' [IEEE Standard Computer Dictionary, 1991, https://doi.org/10.1109/IEEESTD.1991.106963]",
			"skos:example":[
				"format conversion tool",
				"MagresView library"
			],
			"skos:narrower":[
				"psdiVoc:psdiDeliverable"
			],
			"skos:Collection":"psdiVoc:psdiOverviewCollection",
			"skos:inScheme":"psdiVoc:vocabularyConceptScheme"
		},
		{
			"@id":"psdiVoc:guidance",
			"@type":"skos:Concept",
			"skos:prefLabel":"guidance",
			"skos:definition":"Advice/information made available to PSDI users and contributors via the PSDI platform to provide guidance* about PSDI, its resources and data management and reproducibility principals. *'Guidance, N., Sense 2.' - 'Something which guides or leads.' [Oxford English Dictionary, Oxford UP, December 2023, https://doi.org/10.1093/OED/8957051667]",
			"skos:example":[
				"knowledgebase",
				"support",
				"helpdesk",
				"FAQs",
				"known issue",
				"policy",
				"training resource"
			],
			"skos:narrower":[
				"psdiVoc:psdiDeliverable"
			],
			"skos:Collection":"psdiVoc:psdiOverviewCollection",
			"skos:inScheme":"psdiVoc:vocabularyConceptScheme"
		},
		{
			"@id":"psdiVoc:metadata",
			"@type":"skos:Concept",
			"skos:prefLabel":"metadata",
			"skos:definition":"The PSDI metadata is 'data about data' that describes the PSDI platform and its resources, data, services and guidance to facilitate its access and document its provenance.",
			"rdf:type":"obo:IAO_0000120",
			"skos:narrower":[
				"psdiVoc:psdiDeliverable"
			],
			"skos:Collection":"psdiVoc:psdiOverviewCollection",
			"skos:inScheme":"psdiVoc:vocabularyConceptScheme"
		},
		{
			"@id":"psdiVoc:resource",
			"@type":"skos:Concept",
			"skos:prefLabel":"resource",
			"skos:definition":"A set of data and/or services and/or tools and/or guidance related to the Physical Sciences that can be accessed through the PSDI platform and has been developed by a PSDI partner (e.g. as part of a Pathfinder).",
			"rdf:type":"dcat:Catalog",
			"skos:related":[
				"psdiVoc:pathfinder"
			],
			"skos:narrower":[
				"psdiVoc:psdiDeliverable"
			],
			"skos:Collection":"psdiVoc:psdiOverviewCollection",
			"skos:inScheme":"psdiVoc:vocabularyConceptScheme"
		},
		{
			"@id":"psdiVoc:data",
			"@type":"skos:Concept",
			"skos:prefLabel":"data",
			"skos:definition":"Physical Sciences data associated with a resource, and available for access or download through the PSDI platform.",
			"rdf:type":"dcat:Dataset",
			"skos:example":[
				"database",
				"dataset",
				"data collection",
				"repository",
				"catalogue"
			],
			"skos:narrower":[
				"psdiVoc:psdiDeliverable"
			],
			"skos:Collection":"psdiVoc:psdiOverviewCollection",
			"skos:inScheme":"psdiVoc:vocabularyConceptScheme"
		},
		{
			"@id":"psdiVoc:communityActivity",
			"@type":"skos:Concept",
			"skos:prefLabel":"communityActivity",
			"skos:definition":"Activity* organised as part of the PSDI project to support and build up its community of users and contributors and foster collaborations related to it. *' Activity, N., Sense 3.a.' - 'Something which a person, animal, or group chooses to do; an occupation, a pursuit. Frequently in plural.' [Oxford English Dictionary, Oxford UP, July 2023, https://doi.org/10.1093/OED/5595022164]",
			"skos:example":[
				"event",
				"conference",
				"town hall meeting",
				"training course",
				"news"
			],
			"skos:narrower":[
				"psdiVoc:psdiDeliverable"
			],
			"skos:Collection":"psdiVoc:psdiOverviewCollection",
			"skos:inScheme":"psdiVoc:vocabularyConceptScheme"
		}
	]
}
